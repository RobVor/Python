   "source": [
    "from bokeh.charts import Scatter, output_file, show\n",
    "import pandas\n",
    "\n",
    "df = pandas.DataFrame(columns=[\"X\",\"Y\"])\n",
    "df[\"X\"]=[1,2,3,4,5]\n",
    "df[\"Y\"]=[5,6,4,5,3]\n",
    "\n",
    "plot = Scatter(df, x=\"X\",y=\"Y\", title=\"Temp Observation\", xlabel=\"Day\", ylabel=\"Temp\")\n",
    "\n",
    "output_file(\"ScatterPlot.html\")\n",
    "show(plot)"
   ]
  }
